[
  {
    "key": "alt-.",
    "command": "editor.action.goToTypeDefinition"
  },
  {
    "key": "alt-,",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "ctrl+[",
    "command": "extension.vim_escape"
  },
  {
    "key": "ctrl+n",
    "command": "workbench.action.quickOpenSelectNext",
