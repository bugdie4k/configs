{
  "sqltools.highlightQuery": false,
  "editor.tabSize": 2,
  "window.zoomLevel": 1,
  "editor.fontSize": 13,
  "terminal.integrated.fontSize": 12.5,
  "workbench.activityBar.visible": false,
  "window.menuBarVisibility": "toggle",
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  // "vim.easymotionDimBackground": false,
  "vim.easymotionMarkerForegroundColorOneChar": "#ff0000",
  "vim.easymotionMarkerForegroundColorTwoChar": "#ffb400",
  "vim.camelCaseMotion.enable": true,
  "vim.hlsearch": true,
  "window.titleBarStyle": "native",
  "workbench.editor.showTabs": true,
  "workbench.editor.enablePreview": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "workbench.statusBar.visible": true,
  "trailing-spaces.trimOnSave": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.formatOnSave": true,
  // "editor.formatOnSaveMode": "modifications",
  "editor.renderWhitespace": "trailing",
  "trailing-spaces.highlightCurrentLine": false,
  "trailing-spaces.syntaxIgnore": ["markdown"],

  "editor.parameterHints.cycle": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  // "editor.defaultFormatter": "samverschueren.linter-xo",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features"
    // "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[html]": {
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "[jsonc]": {
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "%"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    }
  ],
  "sassFormat.indent": 2,
  "prettier.semi": false,
  "prettier.arrowParens": "always",
  "files.autoSave": "off",
  // "editor.codeActionsOnSave": {
  //   // "source.fixAll.eslint": true
  //   "source.fixAll.prettier"
  // },
  "aws.profile": "profile:mftl",
  "html.mirrorCursorOnMatchingTag": false,
  "editor.semanticHighlighting.enabled": false,
  "shellformat.flag": "-ci",
  "xo.enable": true,
  "xo.format.enable": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "yaml.customTags": [
    "!Split sequence",
    "!If sequence",
    "!And sequence",
    "!Not sequence",
    "!Condition scalar",
    "!Ref scalar",
    "!GetAtt scalar",
    "!Sub scalar",
    "!Sub sequence",
    "!Equals sequence",
    "!Not sequence",
    "!FindInMap sequence",
    "!Join sequence"
  ],
  "turboConsoleLog.logMessagePrefix": "df",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.quote": "'"
}
